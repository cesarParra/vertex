{
  "name": "vrtx",
  "version": "0.5.0",
  "author": "Cesar Parra",
  "bin": {
    "vrtx": "./bin/run"
  },
  "bugs": "https://github.com/cesarParra/vrtx/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^3.1.0",
    "@types/mkdirp": "^1.0.1",
    "antlr4ts": "^0.5.0-alpha.3",
    "mkdirp": "^1.0.4",
    "prettier": "^2.0.5",
    "prettier-plugin-apex": "^1.5.0",
    "rimraf": "^3.0.2",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.11",
    "@types/jest": "^26.0.0",
    "@types/node": "^10.17.26",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "antlr4ts-cli": "^0.5.0-alpha.3",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "globby": "^10.0.2",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/cesarParra/vrtx",
  "keywords": [
    "vertex", "vrtx", "apex", "salesforce", "language"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "vrtx",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "cesarParra/vrtx",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest --coverage",
    "version": "oclif-dev readme && git add README.md",
    "build": "rimraf ./lib && npm run lint && tsc",
    "antlr4ts": "antlr4ts -visitor src/antlr/VertexLexer.g4 src/antlr/VertexParser.g4 -o src/parser/base/",
    "lint": "eslint --fix --ext .ts ./src/"
  },
  "types": "lib/index.d.ts"
}
